<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_fondo_primario"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:gravity="center"
    android:weightSum="100">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_h2"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/ll_realizar_pedido"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">
                    <TextView
                        android:text="Realizar pedido"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:textSize="@dimen/text_h5"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_consultar_pedido"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_h0"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">
                    <TextView
                        android:text="Consultar pedido"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:textSize="@dimen/text_h5"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_rastrear_pedido"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_h0"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">
                    <TextView
                        android:text="Rastrear pedido"
                        android:layout_centerVertical="true"
                        android:layout_width="wrap_content"
                        android:textSize="@dimen/text_h5"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_reportes"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_h0"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="Reportes"
                        android:textSize="@dimen/text_h5" />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_pagos"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_h0"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="Pagos"
                        android:textSize="@dimen/text_h5" />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_ayuda"
            android:clickable="true"
            android:background="@drawable/fondo_boton_grande_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_h0"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_h2">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="15">
                    <ImageView
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"

                        android:src="@drawable/ic_upload"
                        android:layout_width="@dimen/text_h3"
                        android:layout_height="@dimen/text_h3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="85">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="Ayuda"
                        android:textSize="@dimen/text_h5" />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </LinearLayout>



</LinearLayout>
